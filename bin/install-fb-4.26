#!/usr/bin/env bash

echo "Installing FBAudienceNetwork-4.26.0"

mkdir tmp/

pushd tmp/
curl -O -J -L https://origincache.facebook.com/developers/resources/?id=FBAudienceNetwork-4.26.0.zip
unzip FBAudienceNetwork-4.26.0.zip
popd

sudo cp tmp/FBAudienceNetwork.framework/Headers/* Pods/FBAudienceNetwork/FBAudienceNetwork.framework/Headers/
sudo cp tmp/FBAudienceNetwork.framework/FBAudienceNetwork Pods/FBAudienceNetwork/FBAudienceNetwork.framework/

rm -rf tmp

echo "Done Installing FBAudienceNetwork-4.26.0"